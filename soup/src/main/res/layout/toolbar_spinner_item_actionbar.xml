<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="200dp"
    android:layout_height="wrap_content">

    <TextView
        android:id="@android:id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawablePadding="@dimen/material_padding"
        android:fontFamily="sans-serif"
        android:paddingLeft="@dimen/material_padding_double"
        android:paddingStart="@dimen/material_padding_double"
        android:paddingRight="@dimen/material_padding_half"
        android:paddingEnd="@dimen/material_padding_half"
        android:textColor="@color/white"
        android:textSize="@dimen/material_type_toolbar_spinner"
        android:textStyle="bold"
        tools:ignore="UnusedAttributes" />

    <!--android:drawableRight="@drawable/spinner_triangle"-->
    <!--android:drawableEnd="@drawable/spinner_triangle"-->
</LinearLayout>